                box.appendChild(msg)
                var interval = setInterval(function() {
                    i--
                    msg.innerHTML = '倒计时' + i + '秒'
                    if (i === 0) {
                        window.open(url + separate + 'FbmNv=' + token , '_self')
                        clearInterval(interval)
