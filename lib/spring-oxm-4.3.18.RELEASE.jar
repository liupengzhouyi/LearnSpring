    <script type="text/javascript">
        function mycount_add_count(stype){
              var img=new Image();
              var path='';
              if (window.location.search == "")
                path = window.parent.location.search;
              else
                path = window.location.search;
              
              img.src="/taste.gif"+path+'&stype='+stype+'&_t='+(new Date().getTime());
              var bodyed = document.getElementsByTagName("body");
              if(bodyed.length){
                img.style.display="none";
                    bodyed[0].appendChild(img);
              }
        }
